{
  "name": "boooklove",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "node scripts/kill-port.mjs 3000 && next dev -p 3000",
    "build": "prisma generate && next build",
    "start": "node scripts/kill-port.mjs 3000 && next start -p 3000",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate deploy",
    "prisma:seed": "tsx prisma/seed.ts",
    "patch:files": "node scripts/queue-files.mjs",
    "patch:dry": "bash scripts/self-apply-patch.sh --patch ./changes.patch --dry-run",
    "patch:apply": "bash scripts/self-apply-patch.sh --patch ./changes.patch",
    "patch:reverse": "bash scripts/self-apply-patch.sh --patch ./changes.patch --reverse",
    "patch:stdin": "bash scripts/self-apply-patch.sh --patch -",
    "patch:root": "bash scripts/self-apply-patch.sh --root .",
    "ai:snapshot": "bash scripts/make-ai-snapshot.sh",
    "verify:legacy": "tsx scripts/mark-legacy-verified.ts"
  },
  "dependencies": {
    "@livekit/components-react": "^2.9.15",
    "@livekit/components-styles": "^1.1.6",
    "@prisma/client": "^6.16.2",
    "@vercel/blob": "^2.0.0",
    "bcryptjs": "^2.4.3",
    "clsx": "^2.1.1",
    "dotenv": "^17.2.2",
    "fontkit": "^2.0.2",
    "livekit-client": "^2.15.14",
    "livekit-server-sdk": "^2.14.0",
    "lucide-react": "0.446.0",
    "next": "14.2.5",
    "next-auth": "5.0.0-beta.20",
    "nodemailer": "^6.10.1",
    "pdfkit": "^0.13.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "7.51.5",
    "recharts": "^3.3.0",
    "sharp": "^0.33.5",
    "swr": "^2.3.6",
    "zod": "3.23.8"
  },
  "devDependencies": {
    "@types/bcryptjs": "2.4.6",
    "@types/node": "^20.19.17",
    "@types/nodemailer": "^7.0.2",
    "@types/pdfkit": "^0.13.7",
    "@types/react": "18.2.66",
    "@types/react-dom": "18.2.22",
    "autoprefixer": "10.4.19",
    "cross-env": "^10.0.0",
    "detect-port": "^2.1.0",
    "dotenv-cli": "^10.0.0",
    "postcss": "8.4.38",
    "prisma": "^6.16.2",
    "tailwindcss": "3.4.10",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  }
}
